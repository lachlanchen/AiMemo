{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "App.tsx",
    "app.config.ts",
    "src"
  ],
  "extends": "expo/tsconfig.base"
}
